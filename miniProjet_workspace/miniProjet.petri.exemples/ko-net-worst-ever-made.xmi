<?xml version="1.0" encoding="UTF-8"?>
<petriNet:Reseau
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petriNet="http://www.example.org/petriNet"
    name="Eléphant">
  <reseauElements
      xsi:type="petriNet:Place"
      name="Rivière"
      arcs="//@reseauElements.1"/>
  <reseauElements
      xsi:type="petriNet:Arc"
      name=""
      placeToTransition="true"
      place="//@reseauElements.0"
      transition="//@reseauElements.2"/>
  <reseauElements
      xsi:type="petriNet:Transition"
      name="Boire l'eaué"
      arcs="//@reseauElements.1 //@reseauElements.3"/>
  <reseauElements
      xsi:type="petriNet:Arc"
      place="//@reseauElements.4"
      transition="//@reseauElements.2"
      nbTransfered="-2"/>
  <reseauElements
      xsi:type="petriNet:Place"
      name="Sècheresse + Malade"
      nbJetons="-5"
      arcs="//@reseauElements.3"/>
</petriNet:Reseau>
